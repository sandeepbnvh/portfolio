{
	"sap.app": {
		"id": "com.san.portfolio",
		"type": "card",
		"title": "Sample of a List with Bullet Chart",
		"subTitle": "Sample of a List with Bullet Chart",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this Card",
		"info": "Additional information about this Card",
		"description": "A long description for this Card",
		"tags": {
			"keywords": [
				"List",
				"Chart",
				"Card",
				"Sample"
			]
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://list"
		}
	},
	"sap.card": {
		"type": "List",
		"header": {
			"title": "Education",
			"icon": {
				"src": "sap-icon://skills"
			}
			
		},
		"content": {
			"data": {
				"request": {
					"url": "./model/data.json"
				},
				"path": "/skills"
			},
			"maxItems": 3,
			"item": {
				"title": "{name}",
				"description": "{grade}.     {branch}   ({from} - {to})",
		
				"chart": {
					
					"type": "Bullet",
					"minValue": 0,
					"maxValue": "{Expected}",
					"target": "{Target}",
					"value": "{Actual}",
					"displayValue": "{Actual}%",
					"color": "{ChartColor}"
				}
			}
		}
	},
	"list1": {
		"sap.app": {
			"id": "sample.CardsLayout.model.list",
			"type": "card"
		},
		"sap.card": {
			"type": "AdaptiveCard",
			"data": {
				"request": {
					"url": "./model/data.json"
				},
				"path": "/pinfo"
			},
			"header": {
				"title": "{firstname} {lastname}",
				"subTitle": "{position}",
				"icon": {
					"src": "sap-icon://picture"
				}
			},
			"configuration": {
				"editor": "dt/Configuration",
				"parameters": {
					"title" : {
						"value": "Employee ID card"
					},
					"subTitle": {
						"value": "Obligations and Responsibilities"
					}
				}
			},
			"content": {
				"$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
				"type": "AdaptiveCard",
				"version": "1.0",
				"body": [
					{
						"type": "TextBlock",
						"text": "{description}",
						"wrap": true
					}
				]
				
			}
		}
	},
	"list2":{
		"sap.card": {
			"type": "Timeline",
			"header": {
				"title": "Education Summary",
				"icon": {
					"src": "sap-icon://study-leave"
				}
			},
			"content": {
				"data": {
					"request": {
						"url": "./model/data.json"
					},
					"path": "/education"
				},

				"item": {
					"dateTime": {
						"value": "{from}-{to}"
					},
					"description": {
						"value": "Branch : {branch}"
					},
					"title": {
						"value": "{location}."
					},
					"owner": {
						"value": "{name},"
					},
					"text":{
						"value":"Percentage:{percentage}%"
					},
					"icon": {
						"src": "sap-icon://education"
					}
				}
			}
		}
	},
	"donut":{
		"sap.card": {
			"type": "List",
			"width":"30%",
			"header": {
				"title": "Skill Set",
				"icon": {
					"src": "sap-icon://list"
				}
			},
			"content": {
				"data": {
					"request": {
						"url": "./model/data.json"
					},
					"path": "/skills"
				},
				"item": {
					"icon": {
						"src": "{icon}"
					},
					"title": "{Name}",
					"actions": [
						{
							"type": "Navigation",
							"enabled": "{= ${url}}",
							"parameters": {
								"url": "{url}"
							}
						}
					]
				}
			}
		}
	}

	
	
}